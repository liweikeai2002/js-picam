{
  "name": "js-picam",
  "version": "0.0.1",
  "description": "A project for controlling a raspberry pi camera through a web UI.",
  "scripts": {
    "install-ui": "npm install js-picam-ui",
    "build-ui": "cd js-picam-ui && ember build --environment=development --output-path=../public/ui",
    "run-ui": "npm run-script install-ui && npm run-script build-ui",
    "server": "node server.js",
    "build-and-run": "npm run-script run-ui && node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goneplaid/js-picam.git"
  },
  "keywords": [
    "raspberry-pi",
    "camera",
    "javascript",
    "js",
    "express",
    "emberjs",
    "nodejs",
    "python"
  ],
  "author": "Shawn Wilson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/goneplaid/js-picam/issues"
  },
  "homepage": "https://github.com/goneplaid/js-picam#readme",
  "dependencies": {
    "express": "^4.16.2",
    "js-picam-ui": "file:js-picam-ui"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "mocha": "^5.0.0",
    "sinon": "^4.2.2"
  }
}
